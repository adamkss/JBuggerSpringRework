{
	"sap.ui5": {
		"rootView" : {
			"viewName": "jbuggerSAPUI.view.ManageBugs",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"libs": {
				"sap.m": {
				},
				"sap.f":{
					
				}
			}
		},
		"config": {
			"fullWidth": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "jbuggerSAPUI.view",
				"controlId": "flexibleColumnLayout",
				"transition": "slide",
				"bypassed": {
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": [
						"viewBugsMaster",
						"detail"
					]
				},
				{
					"pattern": "createBug",
					"name": "createBug",
					"target": "createBug"
				},
				{
					"pattern": "viewBugs/:layout:",
					"name": "viewBugsMaster",
					"target": [
						"viewBugsMaster",
						"detail"
					]
				},
				{
					"pattern": "viewBugs/detail/{bug}/{layout}",
					"name": "detail",
					"target": [
						"viewBugsMaster",
						"detail"
					]
				},
				{
					"pattern": "viewBugs/detailDetail/{supplier}/{layout}",
					"name": "detailDetail",
					"target": [
						"viewBugsMaster",
						"detail",
						"detailDetail"
					]
				}
			],
			"targets": {
				"createBug":{
					"viewName":"ManageBugs"
				},
				"viewBugsMaster": {
					"viewName": "Master",
					"controlAggregation": "beginColumnPages"
				},
				"detail": {
					"viewName": "Detail",
					"controlAggregation": "midColumnPages"
				},
				"detailDetail": {
					"viewName": "DetailDetail",
					"controlAggregation": "endColumnPages"
				},
				"page2": {
					"viewName": "AboutPage",
					"controlAggregation": "endColumnPages"
				}
			}
		}
	}
}